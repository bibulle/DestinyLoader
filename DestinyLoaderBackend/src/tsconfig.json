{
  "compilerOptions": {
    "target": "es6",
    //"target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "exclude": [
    "../node_modules"
  ]
}
